<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Menu" table="menu">
    <id name="nmenuId" type="integer" column="nMenu_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nmenuclase" type="integer" column="nMenuClase" nullable="false"/>
    <field name="cmenunom" type="string" column="cMenuNom" length="150" nullable="false"/>
    <field name="cmenudesc" type="string" column="cMenuDesc" length="150" nullable="false"/>
    <field name="nmenuvalor" type="integer" column="nMenuValor" nullable="false"/>
  </entity>
</doctrine-mapping>
