<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="VenCategoria" table="ven_categoria">
    <id name="ncategoriaId" type="integer" column="nCategoria_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ccategorianom" type="string" column="cCategoriaNom" length="100" nullable="false"/>
    <field name="ccategoriadesc" type="string" column="cCategoriaDesc" length="150" nullable="false"/>
    <field name="ccategoriaest" type="string" column="cCategoriaEst" length="1" nullable="false"/>
  </entity>
</doctrine-mapping>
